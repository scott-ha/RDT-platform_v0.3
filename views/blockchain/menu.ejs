    <p>Welcome to <%= title %></p>
    <button onclick="MovetoReg()">Register</button>
    <button onclick="MovetoLogin()">Login</button>
    <button onclick="Logout()">Logout</button>
    <button onclick="MovetounReg()">unRegister</button>
